{
  "name": "natours",
  "version": "1.0.0",
  "description": "Learning Node, Express & MongoDB",
  "main": "app.js",
  "targets": {
    "main": false
  },
  "browserslist": "last 2 versions",
  "scripts": {
    "start": "cross-env nodemon server.js",
    "start:prod": "cross-env SET NODE_ENV=production nodemon server.js",
    "debug": "ndb server.js",
    "debug:prod": "cross-env SET NODE_ENV=production ndb server.js",
    "watch:js": "parcel watch --no-hmr ./public/js/index.js --dist-dir ./public/js/bundled",
    "build": "parcel build ./public/js/index.js --dist-dir ./public/js/bundled"
  },
  "author": "Chirag Mehta",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.32.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.13.16",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.2",
    "pug": "^3.0.2",
    "regenerator-runtime": "^0.14.0",
    "slugify": "^1.6.6",
    "validator": "^13.9.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "parcel": "^2.9.3",
    "prettier": "^2.8.4",
    "process": "^0.11.10"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
